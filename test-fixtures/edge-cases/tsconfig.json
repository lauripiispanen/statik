{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "node",
    "strict": true,
    "esModuleInterop": true,
    "outDir": "./dist",
    "rootDir": "./src",
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
      "@utils/*": ["src/path-alias/utils/*"],
      "@models/*": ["src/path-alias/models/*"],
      "@components/*": ["src/jsx-tsx/*"]
    }
  },
  "include": ["src/**/*.ts", "src/**/*.tsx"]
}
